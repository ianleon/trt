# Copy to /etc/default/trtllm-openai-8355 and edit as needed.

# Required/important defaults
MODEL=nvidia/NVIDIA-Nemotron-3-Nano-30B-A3B-NVFP4
HOST=0.0.0.0
PORT=8355
DOCKER_IMAGE=nvcr.io/nvidia/tensorrt-llm/release:1.2.0rc6

# Optional auth/token
# HF_TOKEN=hf_xxx

# Optional runtime tuning
MAX_BATCH_SIZE=8
MAX_INPUT_LEN=32768
MAX_SEQ_LEN=32768
TRUST_REMOTE_CODE=1
KV_CACHE_FREE_GPU_MEMORY_FRACTION=0.18
BACKEND=_autodeploy
# TP_SIZE=1
# PP_SIZE=1
# EP_SIZE=1
# TOKENIZER=
# EXTRA_LLM_API_OPTIONS=
EXTRA_SERVE_ARGS=--reasoning_parser nano-v3
